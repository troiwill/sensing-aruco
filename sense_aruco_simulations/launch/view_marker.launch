<launch>
    <!-- Launch Gazebo world -->
    <include file="$(find sense_aruco_simulations)/launch/gazebo.launch"/>

    <!-- Launch the marker -->
    <include file="$(find aruco_marker_description)/launch/spawn_marker_gazebo.launch"/>

    <!-- Launch the FPV camera -->
    <include file="$(find fpv_cam_description)/launch/spawn_camera_gazebo.launch">
        <arg name="z" value="2.0"/>
        <arg name="Y" value="1.5707"/>
        <arg name="P" value="1.5707"/>
    </include>
</launch>
